public class App {
    public static void main(String[] args) throws Exception {

        for (int i = 5; i != 0; i--) {
         
         if (i == 4) {
                System.out.println("aukonah");
                break;
            }

            System.out.println("hehhehehehehhehe");
        
            System.out.println("Beatriz");
        
        }
        int surnameCount= 5;

        do {
            System.out.println("Asuncion");
            surnameCount--;
        } while (surnameCount != 0);
        
        int nicknameCount = 5;
          
        while (nicknameCount != 0) {
            
            nicknameCount--;
            if (nicknameCount == 2) {
                System.out.println("rawrrrrrrrrrrrr");
                continue;
            }
            
            System.out.println("Beyaaaboo"); 
            
        }
    }
}
